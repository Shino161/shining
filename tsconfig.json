{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "@api/*": ["api/*"],
      "@config/*": ["config/*"],
      "@view/*": ["view/*"],
      "@router/*": ["router/*"],
      "@libs/*": ["libs/*"]
    }
  }
}
